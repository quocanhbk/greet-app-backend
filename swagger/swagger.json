          "proficiency",
          "type"
        ]
      },
      "OnboardUserTopicDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      },
      "OnboardUserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "photo": {
            "type": "string"
          },
          "introduction": {
            "type": "string"
          },
          "dateOfBirth": {
            "type": "string"
          },
          "gender": {
            "$ref": "#/components/schemas/UserGender"
          },
          "languages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OnboardUserLanguageDto"
