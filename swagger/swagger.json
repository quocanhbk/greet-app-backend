      "DeleteUserLanguagesDto": {
        "type": "object",
        "properties": {
          "languageIds": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "languageIds"
        ]
      },
      "UpdateUserPreferenceDto": {
        "type": "object",
        "properties": {
          "partnerDescription": {
            "type": "string"
          },
          "locationRange": {
            "type": "number"
          },
          "ageRangeMin": {
            "type": "number"
          },
          "ageRangeMax": {
            "type": "number"
          }
        },
        "required": [
