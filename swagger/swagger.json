        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "GetUserResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "photo": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "string"
          },
          "introduction": {
            "type": "string"
          },
          "dateOfBirth": {
            "type": "string"
          },
          "gender": {
            "$ref": "#/components/schemas/UserGender"
          },
          "languages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserLanguageDto"
            }
          },
          "preference": {
            "nullable": true,
            "allOf": [
              {
                "$ref": "#/components/schemas/UserPreferenceDto"
              }
            ]
          },
          "topics": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserTopicDto"
            }
          }
        },
        "required": [
          "id",
          "name",
          "email",
          "photo",
          "phoneNumber",
          "introduction",
          "dateOfBirth",
          "gender",
          "languages",
          "preference",
          "topics"
        ]
      },
      "UpdateUserInfoDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "photo": {
            "type": "string"
          },
          "introduction": {
            "type": "string"
          },
          "dateOfBirth": {
            "type": "string"
