        "type": "http",
        "description": "Firebase Token"
      }
    },
    "schemas": {
      "StreamUser": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "role": {
            "type": "string"
          },
          "created_at": {
            "type": "string"
          },
          "updated_at": {
            "type": "string"
          },
          "banned": {
            "type": "boolean"
          },
          "online": {
            "type": "boolean"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "role",
          "created_at",
          "updated_at",
          "banned",
          "online",
          "name",
          "email"
        ]
      },
      "SyncStreamTokenResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/StreamUser"
          }
        },
        "required": [
          "token",
          "user"
        ]
      },
      "StreamChannelItem": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "cid": {
            "type": "string"
          },
          "memberCount": {
            "type": "number"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "type",
          "cid",
          "memberCount",
          "name"
        ]
      },
      "GetChannelsResponse": {
        "type": "object",
        "properties": {
          "channels": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/StreamChannelItem"
            }
          }
        },
        "required": [
          "channels"
        ]
      },
      "AddMemberDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string"
          }
        },
        "required": [
          "userId"
        ]
      },
      "SendMessageAs": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        },
        "required": [
          "userId",
          "message"
        ]
      },
      "LanguageItem": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "GetLanguagesResponseDto": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/LanguageItem"
            }
          },
          "total": {
            "type": "number"
          }
        },
        "required": [
          "data",
          "total"
        ]
      },
      "UserGender": {
        "type": "string",
        "enum": [
          "Male",
          "Female",
          "Unknown"
        ]
      },
      "UserLanguageType": {
        "type": "string",
        "enum": [
          "Fluent",
          "Learning"
        ]
      },
      "UserLanguageDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "proficiency": {
            "type": "number"
          },
          "type": {
            "$ref": "#/components/schemas/UserLanguageType"
          }
        },
        "required": [
          "id",
          "name",
          "proficiency",
          "type"
        ]
      },
      "UserPreferenceDto": {
        "type": "object",
        "properties": {
          "partnerDescription": {
            "type": "string"
          },
          "locationRange": {
            "type": "number"
          },
          "ageRangeMin": {
            "type": "number"
          },
          "ageRangeMax": {
            "type": "number"
          }
        },
        "required": [
          "partnerDescription",
          "locationRange",
          "ageRangeMin",
          "ageRangeMax"
        ]
      },
